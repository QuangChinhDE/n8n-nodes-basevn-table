{
	"name": "n8n-nodes-basevn-table",
	"version": "0.1.6",
	"description": "n8n node for BaseVN - App Table API - Manage table records with dynamic column fields",
	"license": "MIT",
	"homepage": "https://github.com/QuangChinhDE/n8n-nodes-basevn-table",
	"keywords": [
		"n8n-community-node-package",
		"basevn",
		"table",
		"records",
		"database",
		"api-integration"
	],
	"author": {
		"name": "QuangChinh",
		"email": "quangchinh@basevn.com"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/QuangChinhDE/n8n-nodes-basevn-table.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "npm run build && npm publish --access public",
		"prepublishOnly": "echo 'Use npm run release'"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/TableManagementApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/TableManagement/TableManagement.node.js",
			"dist/nodes/TableManagement/TableManagementTrigger.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
